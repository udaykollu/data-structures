package basics;

public class sumto1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=0,b=0,c=0,d=0,e=0,f=0,g=0,h=0,i=0,count;
		for( a=1;a<=9;a++)
		{
			
		   for( b=1;b<=9;b++)
		 
		   { 
			if(a==b)
			break;

		    for(c=1;c<=9;c++)
		       {
			   if(b==c)
			   break;
		
		     for(d=1;d<=9;d++)
			   {
		    	 if(c==d)
			  	break;
		 
			    for(e=1;e<=9;e++)
			      {
				  if(e==d)
				  break;
								
			     for(f=1;f<=9;f++)
				     {
			    	 if(e==f)
				     break;

			         for(g=1;g<=9;g++)
				       {
			        	 if(g==f)
				         break;
			            
			        	 for(h=1;h<=9;h++)
				                {
			        		    if(g==h)
				             	break;

			                    for(i=1;i<=9;i++)
			                          {
				                       if(h==i)
					                  break;

					                  count= a/(10*b+c)+d/(10*e+f)+g/(10*h+i);
		                              if(count==1)
		                              {break;}
			                          }

	                            }
			        	 
				       }
				     }
			      }
			   }
		       
		    }
		    
		   }
		   
       }
		
		
	System.out.println("the values are"+a+""+b+""+c+""+d+""+e+""+f+""+g+""+h+""+i);
	}}
